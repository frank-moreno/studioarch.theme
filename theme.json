{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
        "settings": {
            "appearanceTools": true,
            "layout": {
                "contentSize": "1280px",
                "wideSize": "100vw"
            },
            "spacing": {
                "margin": true,
                "padding": true,
                "units": [ "px", "em", "rem", "vh", "vw", "%" ],
                "blockGap": true
            },
            "typography" : {
                "dropCap": true,
                "lineHeight": true,
                "fontWeight": true,
                "fontStyle": true,
                "textTransform": true,
                "letterSpacing": true,
                "textDecoration": true,
                "fluid": true,
                "customFontSize": true,
                "fontSizes":[
                    {
                        "slug": "small",
                        "size": "1.125rem",
                        "name": "Small"
                    },
                    {
                        "slug": "medium",
                        "size": "1.25rem",
                        "name": "Medium"
                    },
                    {
                        "slug": "large",
                        "size": "2rem",
                        "name": "Large"
                    },
                    {
                        "slug": "x-large",
                        "size": "2.75rem",
                        "name": "XL"
                    },
                    {
                        "slug": "huge",
                        "size": "3rem",
                        "name": "Huge"
                    }
                ],
                "fontFamilies": [
                    {
                        "name": "System",
                        "slug": "system",
                        "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
                    },
                    {
                        "name": "Serif",
                        "slug": "serif",
                        "fontFamily": "\"Times New Roman\",\"New York\",Times,\"Noto Serif\",serif"
                    },
                    {
                        "name": "Monospace",
                        "slug": "monospace",
                        "fontFamily": "Consolas,Menlo,Monaco,\"SF Mono\",\"DejaVu Sans Mono\",\"Roboto Mono\",\"Courier New\",Courier,monospace"
                    },
                    {
                        "name": "Nunito",
                        "slug": "nunito",
                        "fontFamily": "Nunito,sans-serif",
                        "fontFace": [
                            {
                                "fontFamily": "Nunito",
                                "fontWeight": "400 700",
                                "fontStyle": "normal",
                                "fontStretch": "normal",
                                "src": [ "file:./assets/fonts/Nunito/Nunito-VariableFont_wdth,wght.ttf" ]
                            },
                            {
                                "fontFamily": "Nunito",
                                "fontWeight": "400 700",
                                "fontStyle": "italic",
                                "fontStretch": "normal",
                                "src": [ "file:./assets/fonts/Nunito/Nunito-Italic-VariableFont_wdth,wght.ttf" ]
                            }
                        ]
                    },
                    {
                        "name": "Poppins",
                        "slug": "poppins",
                        "fontFamily": "Poppins,sans-serif",
                        "fontFace": [
                            {
                                "fontFamily": "Poppins",
                                "fontWeight": "400 900",
                                "fontStyle": "normal",
                                "fontStretch": "normal",
                                "src": [ "file:./assets/fonts/Poppins/Poppins-VariableFont_wght.ttf" ]
                            },
                            {
                                "fontFamily": "Exo 2",
                                "fontWeight": "400 900",
                                "fontStyle": "italic",
                                "fontStretch": "normal",
                                "src": [ "file:./assets/fonts/Poppins/Poppins-Italic-VariableFont_wght.ttf" ]
                            }
                        ]
                    }
                ]
            },
            "border": {
                "radius": true,
                "color": true,
                "style": true,
                "width": true
            },
            "color": {
                "custom": true,
                "customDuotone": true,
                "customGradient": true,
                "duotone": [
                    {
                        "colors": [ "#000", "#FFF" ],
                        "slug": "black-and-white",
                        "name": "Black and White"
                    }
                ],
                "gradients": [],
                "link": false,
                "palette": [
                    {
                        "slug": "orange",
                        "color": "#ff5a3c",
                        "name": "Orange"
                    },
                    {
                        "slug": "light-grey",
                        "color": "#f2f6f7",
                        "name": "Light Grey"
                    },
                    {
                        "slug": "white",
                        "color": "#fff",
                        "name": "White"
                    },
                    {
                        "slug": "dark-blue",
                        "color": "#0a2c3d",
                        "name": "Dark Blue"
                    },
                    {
                        "slug": "black",
                        "color": "#000",
                        "name": "Black"
                    }
                ],
                "text": true,
                "background": true,
                "defaultGradients": true,
                "defaultPalette": true
            },
            "blocks": {
                "core/paragraph": {
                    "color": {},
                    "custom": {},
                    "layout": {},
                    "spacing": {},
                    "typography": {}
                },
                "core/heading": {},
                "etc": {}
            },
            "templateParts": [
                {
                    "name": "header",
                    "title": "Header",
                    "area": "header"
                },
                {
                    "name": "narrow-header",
                    "title": "Narrow Header",
                    "area": "header"
                },
                {
                    "name": "footer",
                    "title": "Footer",
                    "area": "footer"
                },
                {
                    "name": "narrow-footer",
                    "title": "Narrow Footer",
                    "area": "footer"
                },
                {
                    "name": "sidebar",
                    "title": "Sidebar",
                    "area": "uncategorized"
                }
            ],    
            "customTemplates": [
                {
                    "name": "minimal-template",
                    "title": "Minimal Template",
                    "postTypes": [ "post","page" ]
                },
                {
                    "name": "about",
                    "title": "About",
                    "postTypes": [ "post","page" ]
                },
                {
                    "name": "blog",
                    "title": "Blog",
                    "postTypes": [ "post","page" ]
                },
                {
                    "name": "work",
                    "title": "Work",
                    "postTypes": [ "post","page" ]
                },
                {
                    "name": "contact",
                    "title": "Contact",
                    "postTypes": [ "post","page" ]
                },
                {
                    "name": "quote",
                    "title": "Quote",
                    "postTypes": [ "post","page" ]
                },
                {
                    "name": "single-project",
                    "title": "Single Project",
                    "postTypes": [ "post","page, project" ]
                },
                {
                    "name": "basic-template",
                    "title": "Basic Template",
                    "postTypes": [ "post","page" ]
                },
                {
                    "name": "no-title",
                    "title": "No title",
                    "postTypes": [ "post","page" ]
                },
                {
                    "name": "featured-image",
                    "title": "Featured image in header",
                    "postTypes": [ "post","page" ]
                },
                {
                    "name": "sidebar-page-template",
                    "title": "Page with sidebar",
                    "postTypes": [ "page" ]
                },
                {
                    "name": "sidebar-post-template",
                    "title": "Post with sidebar",
                    "postTypes": [ "post" ]
                },
                {
                    "name": "single-product",
                    "title": "WooCommerce single product (Requires WooCommerce)"
                },
                {
                    "name": "archive-product",
                    "title": "WooCommerce product archive (Requires WooCommerce)"
                }
            ]
    },
    "styles": {
    }
}